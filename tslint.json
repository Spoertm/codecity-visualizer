{
    "extends": "tslint:latest",
    "rules": {
        "interface-name": "never-prefix",
        "variable-name": ["check-format", "allow-leading-underscore"],
        "trailing-comma": [true, "never"],
        "no-bitwise": false,
        "quotemark": [false, "single"],
        "eofline": false,
        "no-constructor-vars": true
    }
}
