{
    "include": [
        "lib/**/*.js"
    ],
    "exclude": [
        "node_modules/"
    ],
    "extension": [
        ".js"
    ],
    "reporter": [
        "text-summary",
        "html"
    ],
    "cache": true,
    "sourceMap": true,
    "instrument": true,
    "all": false,
    "check-coverage": true,
    "lines": 80,
    "functions": 80
}